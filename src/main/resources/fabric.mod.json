{
    "schemaVersion": 1,
    "id": "elytra-infinite",
    "version": "${version}",
    "name": "Elytra Infinite",
    "description": "A mod to infinitely fly with an elytra without fireworks.",
    "authors": [
        "meanwhile131"
    ],
    "contact": {
        "email": "meanwhile049@gmail.com",
        "issues": "https://github.com/meanwhile131/elytra-infinite/issues",
        "sources": "https://github.com/meanwhile131/elytra-infinite"
    },
    "license": "MIT",
    "icon": "assets/elytra-infinite/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "meanwhile131.elytrainfinite.ElytraInfinite"
        ],
        "modmenu": [
            "meanwhile131.elytrainfinite.ModMenuIntegration"
        ]
    },
    "mixins": [
        "elytra-infinite.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.18.4",
        "minecraft": "~26.1-snapshot-2",
        "java": ">=25",
        "fabric-api": "*",
        "fabric-key-binding-api-v1": "*",
        "yet_another_config_lib_v3": "*"
    },
    "recommends": {
      "modmenu": ">=17.0.0-alpha.1"
